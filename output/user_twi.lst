                  for(i=0;i<8;i++ )
                {
                      IIC_SCL=0; 
                      delay_us(2);
                  IIC_SCL=1;
                      receive<<=1;
                      if(READ_SDA)receive++;   
                  delay_us(1); 
                  }          
                  if (!ack)
                      IIC_NAck();//发送nACK
